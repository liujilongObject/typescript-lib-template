{
  "name": "your-lib",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "node scripts/prepare.js && husky",
    "dev": "pnpm run clean && pnpm run build:watch",
    "build:watch": "rollup -c -w",
    "build": "pnpm run clean && rollup -c && pnpm run build:types",
    "build:types": "tsc-alias -p tsconfig.json --outDir dist/types && api-extractor run --local --verbose && rimraf dist/types",
    "clean": "rimraf dist",
    "lint": "eslint . --fix && pnpm run format",
    "format": "prettier --write \"src/**/*.{js,ts,mjs}\"",
    "lint-staged": "lint-staged",
    "prepublishOnly": "pnpm run build && node scripts/prepublish.js"
  },
  "keywords": [],
  "author": "liu4426350@qq.com",
  "dependencies": {
    "@babel/runtime": "^7.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@microsoft/api-extractor": "^7.55.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "^20.11.24",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.4",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liujilongObject/typescript-lib-template.git"
  },
  "lint-staged": {
    "src/**/*.{js,ts,mjs}": [
      "pnpm run lint"
    ]
  },
  "license": "MIT"
}
